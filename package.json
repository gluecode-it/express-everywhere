{
  "name": "@gluecode-it/express-everywhere",
  "version": "1.0.0",
  "description": "makes possible to run express locally and in google cloud",
  "main": "dist/src/index.js",
  "scripts": {
    "watch": "tsc -w",
    "postinstall": "tsc"
  },
  "author": "Oliver Lorenz <mail@oliverlorenz.com>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/config-conventional": "9.0.1",
    "@gluecode-it/high-standards": "^1.0.3",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/release-notes-generator": "9.0.1",
    "@types/express": "^4.17.6",
    "@types/node": "^14.0.12",
    "commitlint": "9.0.1",
    "git-authors-cli": "1.0.27",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5"
  },
  "dependencies": {
    "express": "^4.17.1",
    "serverless-express": "^2.0.11",
    "typescript": "^3.9.5"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "git-authors-cli && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,json,css,yaml}": [
      "prettier --write",
      "git add"
    ]
  },
  "contributors": [
    "Oliver Lorenz <mail@oliverlorenz.com>"
  ]
}
