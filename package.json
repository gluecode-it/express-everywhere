{
  "name": "@gluecode-it/express-everywhere",
  "version": "1.0.0",
  "description": "makes possible to run express locally and in google cloud",
  "main": "dist/src/index.js",
  "scripts": {
    "test": "jest --coverage",
    "posttest": "npm run format",
    "dev:test": "jest --coverage --coverage-reporters html --watch-all",
    "prepare": "tsc",
    "format": "prettier --loglevel warn --write \"**/*.{ts,tsx,css,md}\""
  },
  "author": "Oliver Lorenz <mail@oliverlorenz.com>",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1",
    "serverless-express": "^2.0.11"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-typescript": "7.12.1",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@semantic-release/github": "7.1.1",
    "@semantic-release/npm": "^7.0.6",
    "@types/express": "^4.17.8",
    "@types/jest": "26.0.15",
    "@typescript-eslint/eslint-plugin": "4.6.0",
    "@types/node": "14.14.3",
    "@typescript-eslint/parser": "4.5.0",
    "babel-jest": "26.6.1",
    "husky": "4.3.0",
    "jest": "26.6.0",
    "lint-staged": "10.5.0",
    "prettier": "2.1.2",
    "semantic-release": "17.2.1",
    "typescript": "^3.9.7"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "prettier --write"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "contributors": [
    "Oliver Lorenz <mail@oliverlorenz.com>"
  ]
}
